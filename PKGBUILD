pkgname=core-base
pkgver=1.7
pkgrel=1
pkgdesc="base script for core"
arch=('any')
license=('GPL')


package() {
      
       install -Dm755 -t "$pkgdir/usr/bin/" updates
       install -Dm755 -t "$pkgdir/usr/bin/" updates-start
       install -Dm755 -t "$pkgdir/usr/bin/" core-start 
}
